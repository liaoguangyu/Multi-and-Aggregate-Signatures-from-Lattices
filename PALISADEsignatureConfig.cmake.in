# - Config file for the PALISADEsignature package
# It defines the following variables
#  SIGNATURE_INCLUDE_DIRS - include directories for PALISADEsignature
#  SIGNATURE_LIBRARIES    - libraries to link against

get_filename_component(SIGNATURE_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT SIGNATURE_BINARY_DIR)
  include("${SIGNATURE_CMAKE_DIR}/PALISADEsignatureTargets.cmake")
endif()

# These are IMPORTED targets created by PALISADEsignatureTargets.cmake
set(SIGNATURE_INCLUDE "@INSTALL_INCLUDE_DIR@")
set(SIGNATURE_LIBDIR "@INSTALL_LIB_DIR@")
set(SIGNATURE_LIBRARIES @SIGNATURE_PACKAGE_LIBS@ @PALISADE_LIBRARIES@ @THIRDPARTYLIBS@ @OpenMP_CXX_FLAGS@)
set(SIGNATURE_STATIC_LIBRARIES @SIGNATURE_STATIC_LIBS@ @PALISADE_STATIC_LIBRARIES@ @THIRDPARTYLIBS@ @OpenMP_CXX_FLAGS@)
set(SIGNATURE_SHARED_LIBRARIES @SIGNATURE_SHARED_LIBS@ @PALISADE_SHARED_LIBRARIES@ @THIRDPARTYLIBS@ @OpenMP_CXX_FLAGS@)

set(OPENMP_INCLUDES "@OPENMP_INCLUDES@" )
set(OPENMP_LIBRARIES "@OPENMP_LIBRARIES@" )

set(SIGNATURE_CXX_FLAGS "@CMAKE_CXX_FLAGS@ @OpenMP_CXX_FLAGS@")
set(SIGNATURE_C_FLAGS "@CMAKE_C_FLAGS@ @OpenMP_C_FLAGS@")

set (SIGNATURE_EXE_LINKER_FLAGS "@CMAKE_EXE_LINKER_FLAGS@ @OpenMP_EXE_LINKER_FLAGS@")